{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "curly": [2, "multi-line"],
    "no-debugger": 0,
    "eqeqeq": 2,
    "no-eval": 2,
    "wrap-iife": 0,
    "new-cap": 2,
    "no-caller": 2,
    "no-empty": 0,
    "no-new": 0,
    "no-plusplus": 0,
    "no-undef": 2,
    "dot-notation": 0,
    "no-eq-null": 2,
    "no-unused-vars": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2
  }
}
